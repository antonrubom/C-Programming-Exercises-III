#include<stdio.h>

    int main()

{
    FILE * texttoberead;
    int counter = 0;
    char c;
    texttoberead = fopen("input.txt", "r");
    while((c = fgetc(texttoberead)) != EOF) {
        if(c == 'D' || c == 'd')
            counter++;
    }
    printf("Number of D/d = %d\n", counter);
    return 0;
}
